<h1>New Duck<h1>

<% if flash[:errors] %>
    <% flash[:errors].each do |message| %>
        <%= message %>
    <% end %>
<% end %>

<%= form_for(@duck) do |f| %>
    <%= f.label "Name: " %>
    <%= f.text_field :name %>

    <%= f.label "Description: " %>
    <%= f.text_area :description %>

    <%= f.label "Student Owner" %>
    <%= f.collection_select :student_id, Student.all, :id, :name %>

    <%= f.submit "Submit" %>
<% end %>